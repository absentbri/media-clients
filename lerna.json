{
  "command": {
    "bootstrap": {
      "loglevel": "silent"
    },
    "run": {
      "loglevel": "silent",
      "stream": true
    },
    "version": {
      "allowBranch": [
        "master",
        "develop"
      ],
      "conventionalCommits": true,
      "message": "chore(publish): publish"
    }
  },
  "packages": [
    "packages/*"
  ],
  "version": "independent"
}